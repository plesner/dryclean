{
  "name": "Dry Clean",
  "version": "0.1",
  "description": "Checks for tracking cookies",
  "background_page": "background.html",
  "browser_action": {
    "default_title": "Dry Clean",
    "default_popup": "popup.html",
    "default_icon": "cookie.png"
  },
  "permissions": [
    "cookies",
    "webRequest",
    "*://*/*"
  ]
}
